assert: λ(condition)
  if condition
    print(".")
  else
    print("F")

assert_false: λ(condition)
  if condition
    print("F")
  else
    print(".")

assert_equal: λ(item1, item2)
  if item1 ≡ item2
    print(".")
  else
    print("F")

print: λ(str)
  asm
    push 1              ; length of string
    push str.payload    ; string
    push 1              ; file descriptor (STDOUT)
    mov  eax, 4         ; System call number
    sub  esp, 4         ; 4 bytes for OSX
    int  128            ; System Call
    add  esp, 16
  return 0
